// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Parcel {
  id BigInt @id @default(autoincrement())
  handleId String
  image String?
  address String?
  square Float
  price Int?
  ownerAddress  String
  contractAddress String
  tokenId String
  onSale  Boolean
}
model User {
  id BigInt @id @default(autoincrement())
  wallet String
  username String
  email String
  password String
}
model Key {
  id BigInt @id @default(autoincrement())
  
}