// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Parcel {
  id String @id
  image String?
  address String?
  square Float
  price Decimal
  ownerAddress  String
  contractAddress String
  tokenId String
  onSale  Boolean
}
model User {
  id BigInt @id
  wallet String
  name String
  email String
  password String
}